#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../lottie-react-native/node_modules/react-native/local-cli/wrong-react-native.js" "$@"
  ret=$?
else 
  node  "$basedir/../lottie-react-native/node_modules/react-native/local-cli/wrong-react-native.js" "$@"
  ret=$?
fi
exit $ret
